{
  "terraform": {
    "backend": {
      "remote": {
        "organization": "Telecom-Cloudvalley",
        "workspaces": {
          "name": "Telecom"
        }
      }
    }
  },

  "provider": {
    "aws": {
      "region": "us-east-1",
      "access_key": "${var.access_key}",
      "secret_key": "${var.secret_key}",
      "token": "${var.token}"
    }
  },

  "resource": {
    "aws_s3_bucket": {
      "BucketS3ExampleTerraform": {
        "bucket": "${var.bucket_name}"
      }
    }
  },

  "output": {
    "bucket_id": {
      "value": "${aws_s3_bucket.BucketS3ExampleTerraform.id}"
        
    },
    "bucket_arn": {
      "value": "${aws_s3_bucket.BucketS3ExampleTerraform.arn}"
    }
  },

  "variable": {
    "bucket_name": {
      "description": "Bucket s3 name",
      "type": "string",
      "default": "bucket-test-cv-terraform"
    },
    "access_key": {
      "description": "AWS access key",
      "type": "string",
      "default": "ASIA46RFB2C7MBAHEOMF"
    },
    "secret_key": {
      "description": "AWS secret access key",
      "type": "string",
      "default": "Cw++TjJhyqG27OdbF4+C03O2LpyvK6F/25vl6BnU"
    },
    "token": {
      "description": "AWS session token",
      "type": "string",
      "default": ""
    }
  }
}
